<h1>Admin::Users#index</h1>
<p>Find me in app/views/admin/users/index.html.erb</p>


<table class="table">
  <thead>
    <tr>
      <th>id</th>
      <th>name</th>
      <th>email</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= link_to "show", admin_user_path(user), class: "btn btn-outline-primary" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
