<table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th>Title</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% posts.each do |post| %>
      <tr>
        <td><%= link_to post.title, post_path(post) %></td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>